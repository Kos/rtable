{
  "extends": "eslint:recommended",
  "parserOptions": {
    sourceType: "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-unused-vars": [2, {"vars": "local", "args": "none"}],
    "indent": [2, 2],
    "semi": [2, "always"],
    "eqeqeq": [2]
  },
  "env": {
    "browser": true,
    "jquery": true,
    "mocha": true,
    "es6": true
  },
}
